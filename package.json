{
  "name": "elitebuilders",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "validate:api": "ts-node --project tsconfig.scripts.json scripts/validate-api.ts",
    "db:smoke": "ts-node --project tsconfig.scripts.json scripts/db-smoke.ts",
    "db:migrate:profiles": "ts-node --project tsconfig.scripts.json scripts/run-profile-migration.ts",
    "integration:smoke": "ts-node --project tsconfig.scripts.json scripts/integration-smoke.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:changed": "vitest run --related tests",
    "ci": "npm run typecheck && npm run lint && npm run validate:api && npm run test"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.11",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
